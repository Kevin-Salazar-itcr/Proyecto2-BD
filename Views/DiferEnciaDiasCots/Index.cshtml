@model IEnumerable<ProyectoCRM.Models.DiferEnciaDiasCot>

@{
    ViewData["Title"] = "Index";
}

<h1>Top cotizaciones mas largas</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumeroCotizacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreOportunidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreCuenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DíasDeDiferencía)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NumeroCotizacion)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreOportunidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreCuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DíasDeDiferencía)
            </td>
        </tr>
}
    </tbody>
</table>

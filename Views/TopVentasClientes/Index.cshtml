@model IEnumerable<ProyectoCRM.Models.TopVentasCliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Top 10 ventas clientes</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreCuenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VentaTotal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreCuenta)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.VentaTotal)
            </td>
        </tr>
}
    </tbody>
</table>
